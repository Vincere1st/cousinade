{
  "root": true,
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "plugin:vue/vue3-strongly-recommended"
  ],
  "rules": {
    "@typescript-eslint/no-unused-vars": 2,
    "vue/multi-word-component-names": 0,
    "no-console" : 2
  },
  "settings": {
    "import/ignore": [
      "vue"
    ]
  }
}

